{
  "name": "nest-next-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "npm run build:server && npm run build:web",
    "build:web": "rimraf build/client && cd client && cross-env NODE_ENV=production next build",
    "build:server": "cross-env NODE_ENV=production nest build -p server/tsconfig.build.json",
    "prod": "cross-env NODE_ENV=production node build/server/main.js",
    "prod:pm2": "pm2 start build/server/main.js --name demoweb",
    "start": "npm run start:server",
    "start:web": "cd client && cross-env next dev -p 3001",
    "start:server": "cross-env NODE_ENV=development nest start --watch -p server/tsconfig.build.json",
    "start:debug": "nest start --debug --watch -p server/tsconfig.json",
    "format": "prettier --write \"server/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test,server}/**/*.ts\" --fix",
    "lint:server": "eslint \"server/**/*.ts\" --fix",
    "lint:web": "cd client && next lint"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.16.13",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dubbo2.js": "^2.3.9",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "interpret-util": "^0.0.6",
    "jaeger-client": "^3.18.1",
    "js-to-java": "^2.7.0",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-plugin-antd-less": "^1.4.3",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^7.0.5",
    "node-apollo": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "zone-context": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/js-to-java": "^2.7.1",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@umijs/fabric": "^2.8.1",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1"
  }
}
